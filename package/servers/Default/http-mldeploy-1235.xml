<package-http-server xmlns:cts="http://marklogic.com/cts" xmlns="http://marklogic.com/manage/package/servers">
  <config>
    <!-- Documentation can be found here: https://docs.marklogic.com/admin-help/http-server -->
    <server-type>http-server</server-type>
    <group-name>Default</group-name>
    <name>http-mldeploy-1235</name>
    <package-http-properties>
      <root>/</root>
      <port>1235</port>
      <enabled>true</enabled>
      <authentication>digest</authentication>
      <webDAV>false</webDAV>
      <log-errors>true</log-errors>
      <!-- <File-log-level>notice</webDAV> -->
      <!-- <File-log-level>info</webDAV> -->
      <distribute-timestamps>fast</distribute-timestamps>
      <debug-allow>true</debug-allow>
      <url-rewriter>/url-rewriter.xqy</url-rewriter>
      <rewrite-resolves-globally>false</rewrite-resolves-globally>
      <ssl-ciphers>ALL:!LOW:@STRENGTH</ssl-ciphers>
    </package-http-properties>
    <links>
      <group-name>Default</group-name>
      <modules-database>Documents</modules-database>
      <database>Modules</database>
      <default-user>nobody</default-user>
    </links>
  </config>
</package-http-server>
